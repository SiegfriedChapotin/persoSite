                    <td class="text-center"><a class="btn" {{designer.link}}
                                               data-toggle="tooltip" data-placement="top" title="{{ designer.tooltip }}">
                            <i class="{{ designer.debenture }}"></i>
                        </a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>


            <tfoot>
            {% include ('curriculum_vitae/skillsPage/skillsPageGeneric/tfoot.html.twig') %}
            </tfoot>
        </table>
    </div>

